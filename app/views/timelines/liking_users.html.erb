<h3>いいねユーザー一覧</h3>
<table>
	<tbody>
		<% @users.each do |user| %>
		<tr class="liking_users">
			<td><%= image_tag user.profile.image.url(:medium) %></td>
			<td>
		    <% if user.profile.name %>
				<%= link_to user.profile.name, profile_path(user.profile) %>
		    <% else %>
		      <%= link_to user.name, profile_path(user.profile) %>
		    <% end %>
		  </td>
		  <td><%= user.name %></td>
		</tr>
		<% end %>
	</tbody>
</table>